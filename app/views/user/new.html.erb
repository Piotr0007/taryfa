<h1>Create User</h1>

<%= form_for :user, url: users_path do |f| %>
<p>
<%= f.label :name %><br>
<%= f.text_field :name %>
</p>
<p>
<%= f.label :lastname %><br>
<%= f.text_area :lastname %>
</p>
<p>
<%= f.label :email %><br>
<%= f.text_area :email %>
</p>
<p>
<%= f.label :phone %><br>
<%= f.text_area :phone %>
</p>
<p>
<%= f.submit %>
</p>
<% end %>